# Strip the variables as we plop them in their output locations
DEVICE_NAME_IN := $(strip $(DEVICE_NAME))
PARENT_DEVICE_IN :=	$(strip $(PARENT_DEVICE))

$(DEVICE_NAME)_CPU_TYPE_SPEC:=$(strip \
	$(if $(strip $(CPU_TYPE)),\
		$(CPU_TYPE), \
		$(if $(PARENT_DEVICE_IN), \
			$($(PARENT_DEVICE_IN)_CPU_TYPE_SPEC), \
			$($(CPU_TYPE_PENTIUM_AND_HIGHER_SPEC)) \
		) \
	) \
)

$(DEVICE_NAME_IN)_FILES_ELF_SPEC := \
	$(if $(PARENT_DEVICE_IN), $($(PARENT_DEVICE_IN)_FILES_ELF_SPEC),) \
	$(ALL_ELF) \
#

$(DEVICE_NAME_IN)_FILES_PLAIN_SPEC := \
