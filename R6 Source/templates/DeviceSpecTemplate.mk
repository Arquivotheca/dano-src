#

$(DEVICE_NAME_IN)_FILES_DIR_SPEC := \
	$(if $(PARENT_DEVICE_IN), $($(PARENT_DEVICE_IN)_FILES_DIR_SPEC)),) \
	$(ALL_DIR) \
#

$(DEVICE_NAME_IN)_MFLAGS_SPEC := \
	$(if $(PARENT_DEVICE_IN), $(filter-out $($($(PARENT_DEVICE_NAME)_CPU_TYPE_SPEC)_MPAK),$($(PARENT_DEVICE_IN)_MFLAGS_SPEC)),) \
	$($($(DEVICE_NAME)_CPU_TYPE_SPEC)_MPAK) \
	$(ALL_MFLAGS) \
#

$(DEVICE_NAME_IN)_DFLAGS_SPEC := \
